Targil1:
public class Ex1 
{
	public static void main(String[] args) 
	{
		int num1 = 87;
		int num2 = 92;
		int num3 = 101;
		float avg = (float)(num1+num2+num3)/3;
		System.out.println(avg);
	}
}

